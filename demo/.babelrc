{
  "presets": ["es2015", "es2016", "es2017", "react", "stage-1"],
  "plugins": [
    "syntax-async-functions",
    "transform-object-rest-spread",
    ["pre-style", { "config": "PreStyleConfig.js" }]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "presets": ["airbnb"],
      "plugins": [["__coverage__", { "only": "src/" }]]
    }
  }
}
